    "xterm-addon-web-links": "^0.9.0",
    "@tanstack/react-query": "^5.0.0",
    "zustand": "^4.4.0",
    "react-router-dom": "^6.20.0",
    "lucide-react": "^0.300.0",
    "tailwindcss": "^3.3.0",
